{
    "name": "shrnq",
    "private": true,
    "sideEffects": false,
    "type": "module",
    "imports": {
        "#*": "./*"
    },
    "scripts": {
        "build": "remix build",
        "d1:init": "npm run migrations:apply && wrangler d1 execute shrnq_db --file='./fixtures/data.sql'",
        "d1:new": "wrangler d1 create shrnq_db",
        "dev": "remix dev --manual -c \"npm run start\"",
        "format": "prettier --log-level warn --write .",
        "lint": "eslint --max-warnings 0 --fix .",
        "local:d1:init": "npm run local:migrations:apply && wrangler d1 execute shrnq_db --file='./fixtures/data.sql' --local",
        "local:migrations:apply": "pnpm run migrations:apply --local",
        "migrations:apply": "wrangler d1 migrations apply shrnq_db",
        "migrations:gen": "drizzle-kit generate:sqlite",
        "migrations:up": "drizzle-kit up:sqlite",
        "start": "wrangler pages dev --compatibility-date=2023-12-25 ./public",
        "studio": "./node_modules/.bin/drizzle-kit studio",
        "typecheck": "tsc"
    },
    "dependencies": {
        "@conform-to/react": "0.9.1",
        "@conform-to/zod": "0.9.1",
        "@epic-web/client-hints": "1.2.2",
        "@epic-web/invariant": "1.0.0",
        "@headlessui/react": "2.0.0-alpha.3",
        "@heroicons/react": "2.1.1",
        "@nasa-gcn/remix-seo": "2.0.0",
        "@remix-run/cloudflare": "2.5.0",
        "@remix-run/cloudflare-pages": "2.5.0",
        "@remix-run/css-bundle": "2.5.0",
        "@remix-run/react": "2.5.0",
        "@samialdury/remix-auth-webauthn": "2.0.0",
        "class-variance-authority": "0.7.0",
        "clsx": "2.1.0",
        "cookie": "0.6.0",
        "crypto-js": "3.3.0",
        "drizzle-orm": "0.29.3",
        "intl-parse-accept-language": "1.0.0",
        "is-ip": "5.0.1",
        "isbot": "4.4.0",
        "nanoid": "5.0.4",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "remix-auth": "3.6.0",
        "remix-utils": "7.5.0",
        "tailwind-merge": "2.2.0",
        "zod": "3.22.4"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "4.20231218.0",
        "@remix-run/dev": "2.5.0",
        "@samialdury/config": "6.2.0",
        "@types/cookie": "0.6.0",
        "@types/react": "18.2.47",
        "@types/react-dom": "18.2.18",
        "@typescript-eslint/eslint-plugin": "6.18.1",
        "autoprefixer": "10.4.16",
        "drizzle-kit": "0.20.12",
        "eslint": "8.56.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-jsx-a11y": "6.8.0",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "postcss": "8.4.33",
        "prettier": "3.2.2",
        "tailwindcss": "3.4.1",
        "tailwindcss-animate": "1.0.7",
        "typescript": "5.3.3",
        "wrangler": "3.22.4"
    },
    "engines": {
        "node": ">=20",
        "pnpm": ">=8"
    }
}
